{
    "name": "faustbrian/laravel-traits",
    "description": "Traits for Laravel 5.",
    "keywords": ["laravel", "framework", "Laravel-traits", "Laravel Traits", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.0",
        "faustbrian/support": "^1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "suggest": {
        "danielstjules/stringy": "Require ~2.2 for the HasSlug Trait",
        "hashids/hashids": "Require ~1.0 for the HasHashid Trait",
        "ramsey/uuid": "Require ~3.1 for the HasUuid Trait"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Traits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Traits\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
