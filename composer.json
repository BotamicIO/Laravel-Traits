{
    "name": "draperstudio/laravel-traits",
    "type": "library",
    "description": "Traits for Laravel 5.",
    "keywords": [
        "draperstudio",
        "laravel-traits"
    ],
    "homepage": "https://github.com/DraperStudio/laravel-traits",
    "license": "MIT",
    "authors": [{
        "name": "DraperStudio",
        "email": "hello@draperstudio.tech",
        "homepage": "http://draperstudio.tech",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "draperstudio/support": "^0.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8 || ^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "suggest": {
        "danielstjules/stringy": "Require ~2.2 for the HasSlug Trait",
        "hashids/hashids": "Require ~1.0 for the HasHashid Trait",
        "ramsey/uuid": "Require ~3.1 for the HasUuid Trait"
    },
    "autoload": {
        "psr-4": {
            "DraperStudio\\Traits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DraperStudio\\Tests\\Traits\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
