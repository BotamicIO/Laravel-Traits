{
    "name": "artisanry/traits",
    "description": "Traits for Laravel 5.",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-traits",
        "Laravel Traits"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "faustbrian/support": "^1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "5.3.1",
        "mockery/mockery": "1.2.4",
        "phpunit/phpunit": "6.5.14"
    },
    "suggest": {
        "danielstjules/stringy": "Require ~2.2 for the HasSlug Trait",
        "hashids/hashids": "Require ~1.0 for the HasHashid Trait",
        "ramsey/uuid": "Require ~3.1 for the HasUuid Trait"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Traits\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Traits\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
